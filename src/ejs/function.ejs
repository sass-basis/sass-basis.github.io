<%- include('_layout/_head', { 'title': 'Function' }) %>
<body class="_l-body" data-sticky-footer="true">
<%- include('_parts/_ga') %>
<%- include('_object/_component/_page-effect') %>
<div class="_c-drawer" data-c="drawer">

  <%- include('_object/_project/_drawer') %>

  <div id="container" class="_l-container" data-l="container">
    <%- include('_layout/_header') %>

    <div id="contents" class="_l-contents" data-l="contents">
      <%- include('_object/_project/_hero', { 'title': 'Function' }) %>

      <div class="_c-container">
        <div class="_c-row _c-row--margin-l">
          <div class="_c-row__col _c-row__col--1-1 _c-row__col--lg-3-4">
            <main id="main" class="_l-main" role="main">

              <section class="c-section c-section--border" id="_light">
                <h1 class="c-section__h1">_light</h1>

                <pre><code class="scss">//
// A little brighten
//
// @param   hex  $hex
// @return  hex
//
_light(#000)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_lighter">
                <h1 class="c-section__h1">_lighter</h1>

                <pre><code class="scss">//
// Brighten
//
// @param   hex  $hex
// @return  hex
//
_lighter(#000)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_lightest">
                <h1 class="c-section__h1">_lightest</h1>

                <pre><code class="scss">//
// To very brighten
//
// @param   hex  $hex
// @return  hex
//
_lightest(#000)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_dark">
                <h1 class="c-section__h1">_dark</h1>

                <pre><code class="scss">//
// A little darken
//
// @param   hex  $hex
// @return  hex
//
_dark(#fff)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_darker">
                <h1 class="c-section__h1">_darker</h1>

                <pre><code class="scss">//
// Darken
//
// @param   hex  $hex
// @return  hex
//
_darker(#fff)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_darkest">
                <h1 class="c-section__h1">_darkest</h1>

                <pre><code class="scss">//
// To very darken
//
// @param   hex  $hex
// @return  hex
//
_darkest(#fff)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_hash-search">
                <h1 class="c-section__h1">_hash-search</h1>

                <pre><code class="scss">//
// Return matched key in the hash
//
// @param   string  $key
// @param   hash    $hash
// @return  mixed|null
//
_hash-search(key, hash)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_is-int">
                <h1 class="c-section__h1">_is-int</h1>

                <pre><code class="scss">//
// Checks for a valid int
//
// @param  mixed  $value
// @return  bool
//
_is-int(value)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_is-length">
                <h1 class="c-section__h1">_is-length</h1>

                <pre><code class="scss">//
// Checks for a valid CSS length
//
// @param  mixed  $value
// @return  bool
//
_is-length(value)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_is-null">
                <h1 class="c-section__h1">_is-null</h1>

                <pre><code class="scss">//
// Checks for a valid null
//
// @param  mixed  $value
// @return  bool
//
_is-null(value)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_is-number">
                <h1 class="c-section__h1">_is-number</h1>

                <pre><code class="scss">//
// Checks for a valid number
//
// @param  mixed  $value
// @return  bool
//
_is-number(value)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_px2rem">
                <h1 class="c-section__h1">_px2rem</h1>

                <pre><code class="scss">//
// Return rem from px
//
// @param   px  $px
// @param   px  $base-font-size-px
// @return  rem
//
_px2rem(16px)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_rem2px">
                <h1 class="c-section__h1">_rem2px</h1>

                <pre><code class="scss">//
// Return px from rem
//
// @param   rem  $rem
// @param   px   $base-font-size-px
// @return  px
//
_rem2px(1rem)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_space">
                <h1 class="c-section__h1">_space</h1>

                <pre><code class="scss">//
// Return the margin conforming to the rhythm
//
// @param   int     $coefficient
// @return  length
//
_space(1)</code></pre>
              </section>

              <section class="c-section c-section--border" id="_strip-unit">
                <h1 class="c-section__h1">_strip-unit</h1>

                <pre><code class="scss">//
// Return numeric value that doesn't have the unit.
//
// @param   length  $value  numeric value
// @return  int
//
_strip-unit(10px)</code></pre>
              </section>

            </main>
          </div>
          <div class="_c-row__col _c-row__col--1-1 _c-row__col--lg-1-4">
            <aside id="sub" class="_l-sub" role="complementary">

              <section class="p-local-nav">
                <h1 class="p-local-nav__title">Function</h1>
                <ul class="p-local-nav__menu">
                  <li class="p-local-nav__item"><a href="#_light">_light</a></li>
	                <li class="p-local-nav__item"><a href="#_lighter">_lighter</a></li>
	                <li class="p-local-nav__item"><a href="#_lightest">_lightest</a></li>
	                <li class="p-local-nav__item"><a href="#_dark">_dark</a></li>
	                <li class="p-local-nav__item"><a href="#_darker">_darker</a></li>
	                <li class="p-local-nav__item"><a href="#_darkest">_darkest</a></li>
	                <li class="p-local-nav__item"><a href="#_hash-search">_hash-search</a></li>
	                <li class="p-local-nav__item"><a href="#_is-int">_is-int</a></li>
	                <li class="p-local-nav__item"><a href="#_is-length">_is-length</a></li>
	                <li class="p-local-nav__item"><a href="#_is-null">_is-null</a></li>
	                <li class="p-local-nav__item"><a href="#_is-number">_is-number</a></li>
	                <li class="p-local-nav__item"><a href="#_px2rem">_px2rem</a></li>
	                <li class="p-local-nav__item"><a href="#_rem2px">_rem2px</a></li>
	                <li class="p-local-nav__item"><a href="#_space">_space</a></li>
	                <li class="p-local-nav__item"><a href="#_strip-unit">_strip-unit</a></li>
                </ul>
              </section>

            </aside>
          </div>
        </div>
      </div>
    </div>

    <%- include('_layout/_footer') %>
  </div>
</div>
<%- include('_layout/_foot') %>
</body>
</html>
