{
  "version": "9.6.1",
  "main": "index.html",
  "author": "inc2734",
  "license": "MIT",
  "homepage": "https://sass-basis.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/sass-basis/sass-basis.github.io"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@wordpress/browserslist-config": "^2.6.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "cpy-cli": "^3.1.0",
    "cssnano": "^4.1.10",
    "ejs-cli": "^2.1.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "sass-basis": "^9.6.1",
    "sass-basis-layout": "^8.0.4"
  },
  "scripts": {
    "clean:css": "rimraf public/assets/css && mkdir -p public/assets/css",
    "clean:js": "rimraf public/assets/js && mkdir -p public/assets/js",
    "clean:img": "rimraf public/assets/images && mkdir -p public/assets/images",
    "clean:favicon": "rimraf public/favicon.ico",
    "clean:font": "rimraf public/assets/font && mkdir -p public/assets/font",
    "clean:ejs": "rimraf public/*.html",
    "css:style:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/style.scss -o public/assets/css",
    "css:style:postcss": "postcss public/assets/css/style.css -o public/assets/css/style.min.css -c=postcss.config.js",
    "css:style": "npm-run-all -s css:style:*",
    "css:basis-ie9:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/basis-ie9.scss -o public/assets/css",
    "css:basis-ie9:postcss": "postcss public/assets/css/basis-ie9.css -o public/assets/css/basis-ie9.min.css -c=postcss.config.js",
    "css:basis-ie9": "npm-run-all -s css:basis-ie9:*",
    "css": "npm run clean:css && npm-run-all -p css:*",
    "js:app": "webpack src/js/app.js -o public/assets/js/app.min.js --config webpack.config.js",
    "js": "npm run clean:js && npm-run-all -p js:*",
    "img": "npm run clean:img && cpy '**' '../../public/assets/images' --cwd=src/images --parents",
    "favicon": "npm run clean:favicon && cpy 'src/favicon.ico' 'public'",
    "font": "npm run clean:font && cpy 'node_modules/sass-basis/src/font/**' 'public/assets/font'",
    "ejs": "ejs-cli --base-dir src/ejs '*.ejs' --out public/ --options ejs.json",
    "build": "npm-run-all -p css js img favicon font ejs",
    "browsersync": "browser-sync start -s 'public' -f 'public'"
  }
}
